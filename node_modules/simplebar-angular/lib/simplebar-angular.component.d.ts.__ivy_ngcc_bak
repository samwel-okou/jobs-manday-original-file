import { OnInit, AfterViewInit, ElementRef } from '@angular/core';
import SimpleBar from 'simplebar-core';
export declare class SimplebarAngularComponent implements OnInit, AfterViewInit {
    options: SimpleBar.Options;
    elRef: ElementRef;
    SimpleBar: any;
    constructor(elRef: ElementRef);
    ngOnInit(): void;
    ngAfterViewInit(): void;
    ngOnDestroy(): void;
}
